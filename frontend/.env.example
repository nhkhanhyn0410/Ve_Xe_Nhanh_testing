# API Configuration
VITE_API_URL=http://localhost:5000/api/v1
VITE_WS_URL=ws://localhost:5000

# Environment
VITE_NODE_ENV=development

# Google OAuth
VITE_GOOGLE_CLIENT_ID=your-google-client-id

# Facebook OAuth
VITE_FACEBOOK_APP_ID=your-facebook-app-id

# Google Maps
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# App Configuration
VITE_APP_NAME=Vé xe nhanh
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Hệ thống đặt vé xe khách trực tuyến

# Feature Flags
VITE_ENABLE_OAUTH=true
VITE_ENABLE_GUEST_BOOKING=true
VITE_ENABLE_LOYALTY=true

# Payment
VITE_ENABLE_VNPAY=true
VITE_ENABLE_MOMO=true
VITE_ENABLE_ZALOPAY=true
